{
  "name": "mantine-vite-template",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "prepare": "husky || true",
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "ANALYZE=1 npm run build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "npm run prettier && npm run eslint && npm run stylelint",
    "eslint": "eslint . --cache --cache-location .eslintcache --max-warnings=0 --report-unused-disable-directives",
    "stylelint": "stylelint '**/*.css' --cache",
    "prettier": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "vitest": "vitest run",
    "vitest:coverage": "vitest run --coverage",
    "vitest:watch": "vitest",
    "playwright:test": "playwright test",
    "playwright:test:coverage": "PLAYWRIGHT_COVERAGE=1 playwright test",
    "playwright:report": "c8 report --temp-directory .nyc_output --reporter=lcov --reporter=html --reporter=text-summary --reporter=json-summary --reports-dir coverage/playwright",
    "playwright:clean": "node ./scripts/reset-playwright-coverage.mjs",
    "playwright:ci": "node ./scripts/run-playwright-ci.mjs",
    "test": "npm run typecheck && npm run lint && npm run vitest && npm run build",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "codegen": "graphql-codegen"
  },
  "dependencies": {
    "@mantine/charts": "^8.3.10",
    "@mantine/core": "^8.3.10",
    "@mantine/dates": "^8.3.10",
    "@mantine/form": "^8.3.10",
    "@mantine/hooks": "^8.3.10",
    "@mantine/notifications": "^8.3.10",
    "@tabler/icons-react": "^3.35.0",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@tanstack/react-router": "^1.141.1",
    "dayjs": "^1.11.19",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-error-boundary": "^6.0.0",
    "recharts": "^2.15.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@graphql-codegen/cli": "^6.1.0",
    "@graphql-codegen/typescript": "^5.0.6",
    "@graphql-codegen/typescript-operations": "^5.0.6",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@playwright/test": "^1.57.0",
    "@storybook/react": "^10.1.7",
    "@storybook/react-vite": "^10.1.7",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.15",
    "@vueless/storybook-dark-mode": "^10.0.0",
    "c8": "^10.1.3",
    "eslint": "^9.39.1",
    "eslint-config-mantine": "^4.0.3",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^2.4.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-storybook": "^10.1.7",
    "eslint-plugin-testing-library": "^7.13.6",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.5.0",
    "graphql": "^16.12.0",
    "graphql-codegen-typescript-validation-schema": "^0.18.1",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.7.4",
    "prop-types": "^15.8.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "storybook": "^10.1.7",
    "stylelint": "^16.26.1",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0",
    "vite": "^7.2.7",
    "vite-plugin-istanbul": "^7.2.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.15"
  },
  "packageManager": "yarn@4.7.0",
  "resolutions": {
    "glob": "^10.5.0",
    "js-yaml": "^4.1.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npx eslint --cache --fix",
      "npx prettier --write"
    ],
    "*.css": [
      "npx stylelint --fix"
    ]
  }
}
